{{% handler:TTHandler}}{{!

This is the mustache template file for the tap tracker example.

}}

<div class = "row" style="top-margin:2%">

<table class="large-12 columns" id="storyList" >
		<thead>
			<tr>
				<th>Ranking</th>
				<th>Title</th>
				<th>Type</th>
				<th>Priority</th>
				<th>Estimation</th>
				<th>Epic Link</th>
				<th>Status</th>
				<th></th>
			</tr>
		</thead>
{{#userStories}}
		<tbody>
			<tr>
				<td>1</td>
				<td>{{title}}</td>
				<td>{{type}}</td>
				<td>{{priority}}</td>
				<td>{{estimate}} hours</td>
				<td>{{epicLink}}</td>
				<td>{{status}}</td>
				<td><a href=""class="warning button">Estimate</a>
					<a href=""class="alert button">Delete</a>
				</td>
			</tr>
{{/userStories}}
		</tbody>
	</table>





</div>



<script>
	$("#storyList").DataTable({
		paging:false,
		searching:false,
		info:false
	});
</script>
