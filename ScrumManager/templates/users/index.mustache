{{% handler: index_handler }}{{!

	This is the mustache template file for the tap tracker example.
	
}}

<!-- Content of side menu -->
<div class="large-12 columns">
{{#userList}}
<div class="row collapse user-list-column" data-equalizer>
<div class="small-8 large-8 columns" data-equalizer-watch>
<a href="/users/{{username}}">
{{name}}<br>
<span style="font-size: small">{{email}}</span>
</a>
</div>
<div class="small-4 large-4 columns text-center user-list-delete-button"">
<a method="post" href="/users/{{username}}/edit" style="display:{{visibility}}">
Edit
</a>
<a method="post" href="/users/{{username}}/activate" style="display:{{isActive}}">
Activate
</a>
<a method="post" href="/users/{{username}}/deactivate" style="display:{{isUnActive}}">
Deactivate
</a>
<a method="post" href="/users/{{username}}/delete" style="display:{{visibility}}">
<i class="icon-trash"></i>
</a>
<img src={{profilePictureURL}} class="show-profile-image">
</div>


</div>
{{/userList}}
</div>
</div>





