{{% handler:TTHandler}}{{!

	This is the mustache template file for the tap tracker example.
	
}}


<div class="callout large primary">
<div class="row column text-center">
<h1>Blog</h1>
<h2 class="subheader">Browser Users</h2>
</div>
</div>

<div class="row text-center profile-top-bar">
<div class="row">
<div class ="small-4 large-4 small-centered large-centered columns">
<img class="profile-top-bar-img" src="default.jpg">
</div>
<div class = "small-12 large-12 small-centered large-centered  columns">
<span></span>
<span class="profile-top-bar-name">Fagan Ooi</span><br>
<span class="profile-top-bar-roles"><img class="profile-top-bar-roles-icon" src="position.svg"> Programmer/Scrum Member</span>
</div>
</div>
</div>
<div class="row">
<!-- Side menu for user management -->
<div class="large-2 columns">
<div class="row text-center user-management-side-menu">
<div class="columns user-management-side-menu-content"><a href="profile.html">User Profile</a></div>
<div class="columns user-management-side-menu-content"><a href="new.html">Create User</a></div>
<div class="columns user-management-side-menu-content"><a href="edit.html">User Settings</a></div>
<div class="columns user-management-side-menu-content"><a href="search.html">Browser User</a></div>
</div>

</div>


<!-- Content of side menu -->
<div class="large-10 columns">
<br>
<form class="has-form">
<div class="row collapse" style="padding-left: 10%;padding-right: 10%;">
<div class="small-10 large-11 columns">
<input type="text" id="searchUserTextField" placeholder="Search User" name="searchUserTextField"/>
</div>

<div class="small-2 large-1 columns">
<a class="button expand">Search</a>
</div>
</div>
</form>
//Repeat
<div class="row collapse" data-equalizer>
<div class="small-8 large-8 columns userSearchResult" data-equalizer-watch>
<a href="#"> // Has Link to particular user profile: Fagan Ooi</a>
</div>

<div class="small-4 large-4 columns collapse" data-equalizer-watch>
<button id="edit{{user_id}}" class="userEditButton">Edit</button> &nbsp;
<button id="remove_{{user_id}}" class="userRemoveButton">Remove</button>
</div>

</div>
</div>
</div>
</div>









