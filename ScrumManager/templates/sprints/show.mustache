<div class="row medium-8 large-7 columns">

{{#sprint}}
    <article class="blog-post">

        <a href="#"><h3>{{title}}</h3></a>
        <p>{{body}}</p>

    <h3>User Stories</h3>
    {{#userstory}}

        {{#user}}
