{{% handler:TTHandler}}{{!

This is the mustache template file for the tap tracker example.

}}

<div class="row medium-8 large-7 columns">

    {{#sprints}}

<table id="sprintTaskList" style="width:100%">
<thead>
<tr>
<td>Sprint</td>
<td>Description</td>
<td>Expected Duration</td>
<td>Status</td>
</tr>
</thead>
<tbody>
{{#tasks}}
<tr>
<td><a href="{{urlPath}}"><h3>Sprint {{index}}</h3></a></td>
<td>{{body}}</td>
<td>{{duration}}</td>
<td>{{status}}</td>
</tr>
{{/tasks}}
</tbody>


</table>
    {{/sprints}}
</div>

<script>
$("#sprintTaskList").DataTable({
paging:false,
searching:true,
autowidth:false,
info:false
});


</script>

